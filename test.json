package com.takeone.controller;
	
import java.util.List;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Controller;
import org.springframework.ui.ModelMap;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RequestMethod;

import com.takeone.domain.Member;
import com.takeone.service.AmazonShopService;
import com.takeone.service.amazon.Item;
import com.takeone.service.amazon.Request;

@Controller
public class AmazonShopController {

	@Autowired
	private AmazonShopService amazonShopService;
	
	@RequestMapping(value="/amazonshop.do", method=RequestMethod.GET)
	public String getAmazonShopController(
			HttpServletRequest request,
			HttpServletResponse response,
			ModelMap model) {

		List<Member> members = amazonShopService.getList();
		
		model.addAttribute("count", members.size());
		model.addAttribute("members", members);
		
		com.takeone.service.amazon.Request re = new Request("AKIAJFTHACR626UGVXIQ", "7w2iM2OgmQ6oZNnA0zUvHVpYRfgiZTCKpmv/KTdm");
		List<Item> items = re.itemSearch("girl's generation");
		model.addAttribute("items", items);
		
//		 System.out.println(re.itemSearch("girl's generation").size());
		
		return "amazonshop";
	}
	
	@RequestMapping(value="/amazonshop", method=RequestMethod.POST)
	public String postAmazonShopController(String name) {
		amazonShopService.writeMember(name);
		return "redirect:amazonshop.do";
	}	
}
